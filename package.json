{
  "name": "emotion-box",
  "version": "1.0.2",
  "description": "a collection of css-in-js flexbox layout components",
  "main": "lib/index.js",
  "author": "hemlok",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "private": false,
  "keywords": [
    "emotion",
    "css-in-js",
    "css-grid",
    "layouts",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hemlok/emotion-grid"
  },
  "scripts": {},
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "jsxBracketSameLine": true
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "env": {
      "production": {
        "ignore": [
          "*.test.js"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "typescript-eslint-parser",
    "extends": "react-app",
    "plugins": [
      "prettier",
      "typescript"
    ],
    "root": true,
    "rules": {
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@types/react": "^16.4.14",
    "create-emotion": "^9.2.6",
    "emotion": "^9.2.10",
    "eslint-plugin-typescript": "^0.12.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-emotion": "9.2.10",
    "tslint-config-prettier": "^1.15.0",
    "typescript-eslint-parser": "^18.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "typescript": "^3.0.3"
  }
}
